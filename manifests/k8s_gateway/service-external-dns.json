{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "k8s.joshcorp.co/app": "k8s_gateway"
    },
    "name": "external-dns"
  },
  "spec": {
    "ports": [
      {
        "name": "udp-1053",
        "port": 53,
        "protocol": "UDP",
        "targetPort": 1053
      }
    ],
    "selector": {
      "k8s-app": "excoredns"
    },
    "type": "LoadBalancer"
  }
}
