{
  "apiVersion": "gateway.networking.k8s.io/v1",
  "kind": "Gateway",
  "metadata": {
    "labels": {
      "k8s.joshcorp.co/app": "ingress"
    },
    "name": "http"
  },
  "spec": {
    "gatewayClassName": "eg",
    "listeners": [
      {
        "allowedRoutes": {
          "namespaces": {
            "from": "All"
          }
        },
        "name": "http",
        "port": 80,
        "protocol": "HTTP"
      }
    ]
  }
}
