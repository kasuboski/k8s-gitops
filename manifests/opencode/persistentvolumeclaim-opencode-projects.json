{
  "apiVersion": "v1",
  "kind": "PersistentVolumeClaim",
  "metadata": {
    "labels": {
      "app": "opencode",
      "app.kubernetes.io/name": "opencode",
      "k8s.joshcorp.co/app": "opencode"
    },
    "name": "opencode-projects"
  },
  "spec": {
    "accessModes": [
      "ReadWriteMany"
    ],
    "resources": {
      "requests": {
        "storage": "1Mi"
      }
    },
    "storageClassName": "manual",
    "volumeName": "storage"
  }
}
