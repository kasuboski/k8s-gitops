{
  "apiVersion": "v1",
  "kind": "PersistentVolumeClaim",
  "metadata": {
    "labels": {
      "k8s.joshcorp.co/app": "media"
    },
    "name": "adel-speicher"
  },
  "spec": {
    "accessModes": [
      "ReadWriteOnce"
    ],
    "resources": {
      "requests": {
        "storage": "100Gi"
      }
    },
    "storageClassName": "local-storage",
    "volumeName": "adel-speicher"
  }
}
