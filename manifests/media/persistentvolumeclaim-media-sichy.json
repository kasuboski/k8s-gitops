{
  "apiVersion": "v1",
  "kind": "PersistentVolumeClaim",
  "metadata": {
    "labels": {
      "k8s.joshcorp.co/app": "media"
    },
    "name": "media-sichy"
  },
  "spec": {
    "accessModes": [
      "ReadWriteMany"
    ],
    "resources": {
      "requests": {
        "storage": "1Mi"
      }
    },
    "storageClassName": "manual",
    "volumeName": "sichy"
  }
}
